(declare-rel fail ())
(declare-rel main (Bool Bool Bool Bool Int Int Int Int))
(declare-var p1 Bool)
(declare-var t1 Bool)
(declare-var f1 Bool)
(declare-var h1 Bool)
(declare-var z1 Int)
(declare-var x1 Int)
(declare-var y1 Int)
(declare-var l1 Int)
(declare-var p2 Bool)
(declare-var t2 Bool)
(declare-var f2 Bool)
(declare-var h2 Bool)
(declare-var z2 Int)
(declare-var x2 Int)
(declare-var y2 Int)
(declare-var l2 Int)

(rule (=> (and
  (=> p1 (= z1 1))
  (=> p2 (= z2 1))
  (= t1 (and p1 h1))
  (= t2 (and p2 h2))
  (= f1 (not (and p1 h1)))
  (= f2 (not (and p2 h2)))
  (=> t1 (= x1 1))
  (=> t2 (= x2 1))
  (=> f1 (= x1 z1))
  (=> f2 (= x2 z2))
  (=> p1 (= l1 (+ x1 y1)))
  (=> p2 (= l2 (+ x2 y2))))
  (main p1 p2 h1 h2 y1 y2 l1 l2)))

(rule (=> (and
  (=> (and p1 p2) (= y1 y2))
  (main p1 p2 h1 h2 y1 y2 l1 l2)
  (not (=> (and p1 p2) (= l1 l2))))
  fail))

(query fail)
